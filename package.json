    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.3.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.0",
